<!-- Under Carriage -->
<!-- Front Left -->
<!-- select/deslect u/c and wheel wells -->
  <animation>
    <type>select</type>
    <object-name>FLStrut</object-name>
    <object-name>FLHub</object-name>
    <object-name>FLITyre</object-name>
    <object-name>FLOTyre</object-name>
    <object-name>WheelWells</object-name>
    <condition>
      <greater-than>
        <property>gear/gear[0]/position-norm</property>
        <value>0.0</value>
      </greater-than>
    </condition>
  </animation>

<!-- Extend/retract -->
  <animation>
    <type>rotate</type>
    <object-name>FLStrut</object-name>
    <object-name>FLHub</object-name>
    <object-name>FLITyre</object-name>
    <object-name>FLOTyre</object-name>
    <property>gear/gear[0]/position-norm</property>
    <interpolation>
      <entry>
        <ind>0.0</ind>
